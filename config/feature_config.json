{
  "log_features": ["inventory", "price", "moq"],
  "inverse_features": ["leadtime_weeks", "moq", "price"],
  "binary_features": {
    "is_authorized": "source_type == 'Authorized'",
    "has_datasheet": "datasheet is not null",
    "in_stock": "inventory > 0",
    "immediate_availability": "leadtime_weeks == 0"
  },
  "boost_conditions": {
    "ample_stock": {
      "condition": "inventory >= 10 * moq",
      "multiplier": 1.1
    },
    "immediate_ship": {
      "condition": "leadtime_weeks == 0",
      "multiplier": 1.15
    },
    "authorized_source": {
      "condition": "source_type == 'Authorized'",
      "multiplier": 1.05
    }
  }
}